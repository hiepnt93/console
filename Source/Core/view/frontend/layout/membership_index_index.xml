<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
        <css src="Vnecoms_Membership::css/membership.css"/>
    </head>
    <body>
    	<referenceBlock name="head.additional">
	        <block template="membership/head.phtml" class="Vnecoms\Membership\Block\Membership\Head" name="membership.head " />
	    </referenceBlock>
    	
        <referenceContainer name="content">
        	<block class="Vnecoms\Membership\Block\Membership" name="membership.packages" template="membership.phtml" />
        </referenceContainer>
        <referenceBlock name="page.main.title" remove="true" />
    </body>
</page>
